<p id="notice"><%= notice %></p>

<h1>Classtypes</h1>

<table>
  <thead>
    <tr>
      <th>Classname</th>
      <th>Classid</th>
      <th>Cost</th>
      <th>Category</th>
      <th>Classtime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @classtypes.each do |classtype| %>
      <tr>
        <td><%= classtype.classname %></td>
        <td><%= classtype.classid %></td>
        <td><%= classtype.cost %></td>
        <td><%= classtype.category %></td>
        <td><%= classtype.classtime %></td>
        <td><%= link_to 'Show', classtype %></td>
        <td><%= link_to 'Edit', edit_classtype_path(classtype) %></td>
        <td><%= link_to 'Destroy', classtype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Classtype', new_classtype_path %>
